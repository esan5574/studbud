<body class="app-container">
    <nav class="top-nav">
        <ul class="nav-links">
            <li class="nav-item"><a href="#home" class="active"><i class="material-icons" style="87f802c76256a574370c192a687609d0">home</i></a></li>
            <li class="nav-item"><a href="#myboard"> <i class="material-icons" style="8c4c4e3f91b82ffb4d50f84702f1c764">dashboard</i></a></li>
            <li class="nav-item"><a href="#acronymmaker">A</a></li>
        </ul>
